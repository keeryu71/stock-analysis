# API Keys for Financial Data Sources
# Copy this file to .env and add your actual API keys

# Alpha Vantage API Key (free tier available)
# Get your key at: https://www.alphavantage.co/support/#api-key
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here

# Polygon.io API Key (free tier available)
# Get your key at: https://polygon.io/
POLYGON_API_KEY=your_polygon_api_key_here

# IEX Cloud API Key (optional)
# Get your key at: https://iexcloud.io/
IEX_CLOUD_API_KEY=your_iex_cloud_api_key_here

# Quandl API Key (optional)
# Get your key at: https://www.quandl.com/
QUANDL_API_KEY=your_quandl_api_key_here

# Trading Configuration Overrides (optional)
# These will override the default values in config.py
INITIAL_CAPITAL=100000
COMMISSION=0.001
SLIPPAGE=0.0005
RISK_FREE_RATE=0.02

# Database Configuration (for future use)
DATABASE_URL=sqlite:///trading_data.db

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=logs/trading.log

# Email Notifications (for future use)
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
EMAIL_USER=your_email@gmail.com
EMAIL_PASSWORD=your_app_password

# Slack Notifications (for future use)
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK